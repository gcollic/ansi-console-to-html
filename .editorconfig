root = true


[*.{html,sbnhtml,md}]
insert_final_newline = true
end_of_line = lf

[*.csx]
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
end_of_line = lf

[*.fsproj]
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
end_of_line = lf

[*.{fs,fsx,fsi}]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length=100
fsharp_multiline_bracket_style = stroustrup
fsharp_keep_max_number_of_blank_lines=2
